{
  "name": "vladislavs/php-boilerplate-app",
  "type": "project",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "src/",
      "Migrations\\": "migrations/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Vladislavs Poznaks",
      "email": "vladislavs.poznaks@gmail.com"
    }
  ],
  "require": {
    "doctrine/migrations": "^3.6",
    "nikic/fast-route": "^1.3",
    "php-di/php-di": "^7.0",
    "vlucas/phpdotenv": "^5.5",
    "vlucas/valitron": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  }
}
